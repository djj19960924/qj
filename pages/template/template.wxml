<template name="wardrobes">
  <block wx:for="{{list}}">
      <view class="item" bindtap="seeDetail" id="0">
      
          <view>
            <image src="{{item.img}}" style="width:75px;height:58px;"></image>
          </view>
          
          <view class="desc">
            <view class="title">{{item.title}}</view>
            <view class="count">
              <view>{{item.type}}</view>
              <view class="style">{{item.style}}风格</view>

              <view class="pinglun">删除</view>
            </view>
          </view>

      </view>
      <view class="hr2"></view>
  </block>
</template>

